<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="TextBlock" x:Key="FormOfInequalitiesTextBlockDefault">
        <Setter Property="Background" Value="{StaticResource FormOfInequalityColorDefault}"/>
        <Setter Property="Height" Value="25"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="ResultTextBlockDefault">
        <Setter Property="Background" Value="{StaticResource ResultBackgroundColorDefault}"/>
    </Style>
    
</ResourceDictionary>