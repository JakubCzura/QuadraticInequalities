<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="ApplicationBackgroundDefault">White</SolidColorBrush>
    <SolidColorBrush x:Key="InputDescriptionColorDefault">#FFF1A3A3</SolidColorBrush>
    <SolidColorBrush x:Key="FormOfInequalityColorDefault">#FFE0CBCB</SolidColorBrush>
    <SolidColorBrush x:Key="InequalityHeaderColorDefault">#FFC1E56B</SolidColorBrush>

    <LinearGradientBrush x:Key="ResultBackgroundColorDefault" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF69DA52" />
        <GradientStop Color="#FFF6F2F2" Offset="1" />
    </LinearGradientBrush>
</ResourceDictionary>